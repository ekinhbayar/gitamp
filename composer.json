{
  "name": "ekinhbayar/gitamp",
  "description": "Listen ambient music generated by events across github.",
  "keywords": [
    "audio",
    "events",
    "github",
    "async",
    "non-blocking"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Ekin H. Bayar",
      "email": "me@ekins.space",
      "role": "Developer"
    },
    {
      "name": "Pieter Hordijk",
      "email": "info@pieterhordijk.com",
      "homepage": "https://pieterhordijk.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7",
    "psr/log": "^1",
    "amphp/amp": "^2",
    "monolog/monolog": "^2",
    "daverandom/exceptional-json": "^1",
    "nikic/fast-route": "^1",
    "rdlowrey/auryn": "^1",
    "amphp/http-server": "^2.1",
    "amphp/http-client": "^4.5",
    "amphp/websocket-server": "^2.0",
    "ext-json": "*",
    "amphp/http-server-router": "^1.0",
    "amphp/http-server-static-content": "^1.0",
    "amphp/log": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "matthiasmullie/minify": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "ekinhbayar\\GitAmp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ekinhbayar\\GitAmpTests\\": "tests/"
    }
  }
}
